{{#each games as |game|}}

  <!-- <ul> -->
  <h1 data-id="{{game.id}}" class="my-game"> <small>{{game.title}}</small></h1>
  <ul data-id="{{game.id}}">
    Title: {{#if game.title}}
      {{game.title}}
    {{else}}
      No title provided.
    {{/if}}

    <div></div>

  ID: {{#if game.id}}
      {{game.id}}
    {{else}}
      No ID provided.
    {{/if}}
    <div>
    <button type="submit">Remove Game</button>
    <div>
</ul>

{{/each}}
