{{#each games as |game|}}

  <!-- <ul> -->
  <h1 data-id="{{game.id}}" class="my-game"> <small>{{game.title}}</small></h1>
  <ul data-id="{{game.id}}">
    <li>Title: {{#if game.title}}
      {{game.title}}
    {{else}}
      No title provided.
    {{/if}}</li>

    <li>ID: {{#if game.id}}
      {{game.id}}
    {{else}}
      No ID provided.
    {{/if}}</li>
    <button type="submit">Remove Game</button>
</ul>

{{/each}}
